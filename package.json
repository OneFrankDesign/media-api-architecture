{
  "name": "media-api-architecture",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "gen": "bash scripts/gen-proto.sh",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "build": "turbo run build",
    "compose:up": "docker compose -f infra/compose/docker-compose.yml up -d",
